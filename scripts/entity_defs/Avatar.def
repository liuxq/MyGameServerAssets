<root>
	<Volatile>
		<position/>
		<!--<position> 0 </position> Don't update-->
		<yaw/>
		<!--<pitch> 20 </pitch>-->
		<pitch/>
		<roll/>

		<!-- 优化VolatileInfo，关于VolatileInfo可以参考API文档，优化后服务器在确定实体在地面时（navigate）将不同步实体的Y坐标，客户端需要判断实体isOnGround，如果实体在地面则需要做贴地计算。
			在高层建筑内寻路可能需要关闭优化，让服务器同步Y，这样才能精确计算上下层位置。
			(不填默认为true)
		-->
		<optimized> true </optimized>
	</Volatile>

	<Interfaces>
		<Interface>	GameObject		</Interface>
		<Interface>	State			</Interface>
		<Interface>	Motion			</Interface>
		<Interface>	SkillBox		</Interface>
		<Interface>	Combat			</Interface>
		<Interface>	Spell			</Interface>
		<Interface>	Teleport		</Interface>
		<Interface>	AvatarPropertys	        </Interface>
		<Interface>	Dialog			</Interface>
	</Interfaces>

	<Properties>
		<roleType>
			<Type> UINT8 </Type>
			<Flags> BASE </Flags>
			<Persistent> true </Persistent>
		</roleType>
		<roleTypeCell>
			<Type> UINT8 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Persistent> true </Persistent>
		</roleTypeCell>
		<level>
			<Type> UINT16 </Type>
			<Flags> CELL_PUBLIC_AND_OWN </Flags>
			<Persistent> true </Persistent>
		</level>
		<itemList>
			<Type> ITEM_INFO_LIST </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
		</itemList>
		<equipItemList>
			<Type> ITEM_INFO_LIST </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
		</equipItemList>
		<equipWeapon>
			<Type> INT32 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Persistent> true </Persistent>
			<Default>		-1	</Default>
		</equipWeapon>
	</Properties>

	<ClientMethods>
        <ReceiveChatMessage>
        	<Arg>	UNICODE	</Arg>
        </ReceiveChatMessage>

        <onReqItemList>
        	<Arg>   ITEM_INFO_LIST   </Arg>
        	<Arg>   ITEM_INFO_LIST   </Arg>
        </onReqItemList>

        <pickUp_re>
        	<Arg>  ITEM_INFO </Arg>
        </pickUp_re>

        <dropItem_re>
			<Arg> 	ITEM_ID	</Arg>
			<Arg> 	DBID	</Arg>
		</dropItem_re>

		<equipItemRequest_re>
        	<Exposed/>
        	<Arg> ITEM_INFO </Arg>
        	<Arg> ITEM_INFO </Arg>
        </equipItemRequest_re>

		<errorInfo>
			<Arg> 	INT32	</Arg>
		</errorInfo>

	</ClientMethods>

	<BaseMethods>
		<createCell>
			<Arg>		ENTITYCALL					</Arg>
		</createCell>
		<sendChatMessage>
        	<Arg>		UNICODE		</Arg>
        	<Exposed/>
        </sendChatMessage>
        <reqItemList>
        	<Exposed/>
        </reqItemList>
        <pickUpResponse>
        	<Arg>  BOOL </Arg>
        	<Arg>  ENTITY_ID </Arg>
        	<Arg>  ITEM_ID  </Arg>
        	<Arg>  UINT32  </Arg>
        </pickUpResponse>
        <dropRequest>
        	<Exposed/>
        	<Arg> DBID </Arg>
        </dropRequest>
        <swapItemRequest>
        	<Exposed/>
        	<Arg> INT32 </Arg>
        	<Arg> INT32 </Arg>
        </swapItemRequest>
        <equipItemRequest>
        	<Exposed/>
        	<Arg> INT32 </Arg>
        	<Arg> INT32 </Arg>
        </equipItemRequest>
        <updatePropertys>
        </updatePropertys>
        <useItemRequest>
        	<Exposed/>
        	<Arg> INT32 </Arg>
        </useItemRequest>

	</BaseMethods>

	<CellMethods>
		<relive>
			<Exposed/>
			<Arg>	UINT8	</Arg>
		</relive>
		<dropNotify>
			<Arg> 	ITEM_ID	</Arg>
			<Arg> 	DBID	</Arg>
			<Arg> 	UINT32	</Arg>
		</dropNotify>

		<resetPropertys>
		</resetPropertys>
		
		<equipNotify>
			<Arg> 	ITEM_ID	</Arg>
		</equipNotify>
	</CellMethods>

</root>
